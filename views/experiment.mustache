{{>head}} 
    <body class="thesaas-sections-split">
        {{>nav}}
        <header class="header header-inverse bg-fixed" style="background-image: url(https://lh3.googleusercontent.com/a_A54CaTScdJIND5HClRjX1xiSv8jjsm4zzoey39cvH8rHmoqiSoqWmt0jGV9SUv9heTtRhGzPNdhZ11Y5lwqY91Zg)" data-overlay="8">
            <div class="container text-center">
                <div class="row">
                    <div class="col-12 col-lg-8 offset-lg-2">
                        <h1>Update the Groups Info!!</h1>
                    </div>
                </div>
            </div>
        </header>
        <!-- END Header -->
        <main class="main-content">
        <section class="section">
            <div class="container">
                <p class="text-center"><button class="btn btn-lg btn-round btn-danger" data-toggle="modal" data-target="#exampleModal">Launch demo modal</button></p>
                <!-- Modal -->
                <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                ...
                            </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-primary">Save changes</button>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        </main>

{{>footer}}